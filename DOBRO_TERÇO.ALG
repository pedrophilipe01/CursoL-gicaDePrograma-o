Algoritmo "DOBRO_TERÇO"

Var
   numero, dobro, terco: real


Inicio
   escreval("Digite um número: ")
   leia(numero)
   
   dobro <- (numero * 2)
   escreval("O dobro de",numero , " é:", dobro)
   
   terco <- (numero / 3)
   escreval("O terço de",numero , " é:", terco)



Fimalgoritmo