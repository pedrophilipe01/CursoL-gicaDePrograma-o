Algoritmo "DIAS_TRABALHADOS"

Var
   dias_trabalhados,salario :inteiro


Inicio
    escreval("quantos dias você trabalhou em 1 mês?")
    leia(dias_trabalhados)
    

    salario <- dias_trabalhados * 8 * 25
    escreval("o seu salário nesses dias trabalhados será de: ", salario)


Fimalgoritmo