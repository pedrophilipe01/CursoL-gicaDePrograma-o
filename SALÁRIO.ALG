Algoritmo "SALÁRIO"

Var
   nome : caracter
   salario : inteiro


Inicio
   escreval("Digite o nome do funcionário: ")
   leia(nome)
   escreval("Digite o salário do funcionário: ")
   leia(salario)
   
   escreval("O funcionário ", nome, " tem um salário de", salario, " reais em Julho")




Fimalgoritmo